#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <windows.h>
void eles(void);
void quo_WISH(void);
void WISH(int x);
void color(const unsigned short textColor);
void HPNY(void);
void name(void);
int main(void)
{
    srand(time(NULL));
    Sleep(700);
    eles();
    quo_WISH();
    HPNY();
    name();
    return 0;
}

void WISH(int x)
{
    if (x == 0)
        printf("前途无量\n");
    if (x == 1)
        printf("万事大吉\n");
    if (x == 2)
        printf("招财进宝\n");
    if (x == 3)
        printf("十全十美\n");
    if (x == 4)
        printf("财源广进\n");
    if (x == 5)
        printf("身体健康\n");
    if (x == 6)
        printf("事事如意\n");
    if (x == 7)
        printf("心想事成\n");
    if (x == 8)
        printf("大展宏图\n");
    if (x == 9)
        printf("青春永驻\n");
    if (x == 10)
        printf("一帆风顺\n");
    if (x == 11)
        printf("吉庆有余\n");
    if (x == 12)
        printf("五福临门\n");
    if (x == 13)
        printf("三阳开泰\n");
    if (x == 14)
        printf("步步高升\n");
    if (x == 15)
        printf("前程似锦\n");
    if (x == 16)
        printf("平安顺遂\n");
    if (x == 17)
        printf("事业顺利\n");
    if (x == 18)
        printf("喜气洋洋\n");
    if (x == 19)
        printf("财福满门\n");
}
void quo_WISH(void)
{
    int i, j, k = 0, x[20];
    for (i = 0; i < 20; i++)
    {
        color(7);
        printf("\n\n\n\n\n\n\n\n\n\n\n\n\n                                              新年已至，我祝愿您:");
        x[i] = rand() % 20;
        for (j = 0; j < i; j++)
        {
            if (x[j] == x[i])
                x[i] = rand() % 20;
        }
        k = rand() % 15 + 1;
        color(k);
        WISH(x[i]);
        system("cls");
    }
    for (i = 0, j = 0; j < 5;)
    {
        color(7);
        printf("\n\n\n\n\n\n\n\n\n\n\n\n\n                                              新年已至，我祝愿您:");
        k = rand() % 15 + 1;
        color(k);
        WISH(x[i]);
        i++;
        if (i == 20)
        {
            i = 0;
            j++;
        }
        system("cls");
    }
    for (i = 0; i < 3; i++)
    {
        color(12);
        printf("\n\n\n\n\n\n\n\n\n\n\n\n\n                                         新年已至，新的一年里，您必将:");
        WISH(x[i]);
        Sleep(1000);
        system("cls");
    }
}
void color(const unsigned short textColor)
{
    if (textColor >= 0 && textColor <= 15)
        SetConsoleTextAttribute(GetStdHandle(STD_OUTPUT_HANDLE), textColor);
    else
        SetConsoleTextAttribute(GetStdHandle(STD_OUTPUT_HANDLE), 7);
}
void HPNY(void)
{
    color(12);
    printf("\n\n\n\n                         H         H    A A A A A A    P P P P P P    P P P P P P    y         y\n                         H         H    A         A    P         P    P         P    y         y\n                         H         H    A         A    P         P    P         P    y         y\n                         H         H    A         A    P         P    P         P    y         y\n                         H H H H H H    A A A A A A    P P P P P P    P P P P P P    y y y y y y\n                         H         H    A         A    P              P                        y\n                         H         H    A         A    P              P                        y\n                         H         H    A         A    P              P                        y\n                         H         H    A         A    P              P              y y y y y y\n");
    Sleep(1500);
    printf("\n       n n n n n n    E E E E E E    W         W          y         y    E E E E E E    A A A A A A    R R R R R R\n       n         n    E              W         W          y         y    E              A         A    R         R\n       n         n    E              W         W          y         y    E              A         A    R         R\n       n         n    E              W         W          y         y    E              A         A    R         R\n       n         n    E E E E E E    W    W    W          y y y y y y    E E E E E E    A A A A A A    R R R R R R\n       n         n    E              W    W    W                    y    E              A         A    R      R\n       n         n    E              W    W    W                    y    E              A         A    R       R\n       n         n    E              W    W    W                    y    E              A         A    R        R\n       n         n    E E E E E E    W W W W W W          y y y y y y    E E E E E E    A         A    R         R\n");
}
void name(void)
{
    color(7);
    Sleep(2000);
    printf("\n                                                                                                         by 小遛与龟e族\n");
    Sleep(500);
    printf("                                                                                                      2023.1.23 ver.2.1\n\n\n");
    Sleep(2000);
    color(8);
    printf(">");
    Sleep(10);
    printf("程");
    Sleep(10);
    printf("序");
    Sleep(10);
    printf("将");
    Sleep(10);
    printf("在");
    Sleep(10);
    printf("10");
    Sleep(10);
    printf("秒");
    Sleep(10);
    printf("后");
    Sleep(10);
    printf("自");
    Sleep(10);
    printf("动");
    Sleep(10);
    printf("关");
    Sleep(10);
    printf("闭");
    color(7);
    Sleep(10000);
    printf("\n");
}

